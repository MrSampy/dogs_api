{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=DESKTOP-1NKJOPC;Initial Catalog=DogsApi;Integrated Security=True;TrustServerCertificate=True"
